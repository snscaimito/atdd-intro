<html t:type="layout" title="Termin machen"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

    <h1>Termin machen</h1>
    
    <t:form>
    <t:textField t:id="petOwner"/>
    
    <t:select t:id="treatment" t:model="literal:Tollwutimpfung"/>
    
    <t:select t:id="appointmentTime" t:model="literal:15:00"/>
    
    <t:select t:id="appointmentDate" t:model="literal:1.3.11"/>

    <t:textField t:id="petName"/>
    
    <t:submit t:id="bookAppointment" value="Termin buchen"/>
	</t:form>
</html>
